                category === "size" &&
                <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" fill={theme==="light" ?"#2f2f2fa3":"#A7A6A8"} viewBox="0 0 256 256" className="h-[15px] w-[15px]">

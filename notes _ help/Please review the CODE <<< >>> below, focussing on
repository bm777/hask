function resolveHome(filepath) {
  return _babel_runtime_corejs3_core_js_stable_instance_starts_with__WEBPACK_IMPORTED_MODULE_0___default()(filepath).call(filepath, '~') ? path__WEBPACK_IMPORTED_MODULE_14___default().join(os.homedir(), _babel_runtime_corejs3_core_js_stable_instance_slice__WEBPACK_IMPORTED_MODULE_1___default()(filepath).call(filepath, 1)) : filepath;
}
